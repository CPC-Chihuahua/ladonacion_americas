{
    $schema: "http://json-schema.org/draft-07/schema#",
    id: "/url",
    title: "URL",
    description: "A URL in the HTTP or HTTPS protocols",
    type: "string",
    format: "uri",
    pattern: "^https?://(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&/=!,]*)$",
    validate: true,
}
